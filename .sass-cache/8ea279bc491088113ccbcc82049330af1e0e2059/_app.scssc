.seek-ctrl{
	@include col(3);
	float: left;
	@include vertical-align;
	height: 36px;
	overflow: hidden;
	position: relative;

	p{
		font-size: 1.2em;
		display: inline;
		float: right;
		line-height: 0px; //important for lining up seek-line
	}

	#seek-bar{
		@include col(10);
	}
}



.seek-line, .seek-fill, #seek-bar, .seek-buffered{
	@include col(10);
	// @include col-ctr(10);
	height: 0px;
	border-bottom: 1px solid nth($grey, 1);
	// @include vertical-align;
	margin-top:18px;
}



.seek-fill{
	width: 0;
	border-bottom: 1px solid $red;
	position: absolute;
}

.seek-buffered{
	width: 0;
	border-bottom: 1px solid nth($grey, 2);
	position: absolute;
}

#seek-dot{
	width: 12px;
	height: 12px;
	// width: 0px;
	// height: 0px;
	background: $white;
	border-radius: 50%;
	position: absolute;
	top: 13px;
	z-index: 10;
	opacity: .9;
	cursor: pointer;
	// border: 1px solid nth($grey, 2);
}

#seek-dot:active{
	width: 18px;
	height: 18px;
	top: 10px;
}


.playmode-ctrl{
	@include col(1);
	float: left;
	height: 100%;
	padding-left: 2%;

	img{
		@include vertical-align;
		margin-left: 10%;
		cursor: pointer;
		height: 12px;
	}
}

.volume-ctrl{
	@include col(1);
	float: left;
	height: 100%;
	// padding-right: 10%;
	// border-left: 1px solid nth($grey, 1);
	// border-right: 1px solid nth($grey, 1);

	.vol-icon{
		height: 15px;
		cursor: pointer;
		float: left;
		// margin-left: 25%;
		@include vertical-align;
	}

	input{
		@include col(9);
		@include vertical-align;
		display: inline-block;
		margin: 0;
		float: right;
	}

	input[type='range']{
	    -webkit-appearance: none;
	    border: 0;
	    background-color: nth($grey, 2);
	    height: 1px;
	    padding: 0;
	}

	input[type='range']::-moz-range-track {
	    -webkit-appearance: none;
	    border: 0;
	    background-color: nth($grey, 2);
	    height: 1px;
	    padding: 0;
	}

	input[type='range']::-webkit-slider-thumb{
	    -webkit-appearance: none;
	    -moz-appearance: none;
	    width: 6px;
	    height: 6px;
	    -webkit-border-radius: 10px;
	    -moz-border-radius: 10px;
	    border-radius: 10px;
	    background: $white;
	    background-repeat: no-repeat;
	    background-position: 50%;
	}

	input[type='range']::-moz-range-thumb  {
	    -webkit-appearance: none;
	    -moz-appearance: none;
	    width: 6px;
	    height: 6px;
	    -webkit-border-radius: 10px;
	    -moz-border-radius: 10px;
	    border-radius: 10px;
	    background: $white;
	    background-repeat: no-repeat;
	    background-position: 50%;
	}

	input[type='range']::-moz-range-track {
		outline: none;
	}

	input[type='range']::-moz-range-thumb{
		width: 6px;
	    height: 6px;
	    border: none;
	}



}

.playing-share-ctrl{
	@include col(2);
	float: left;
	height: 100%;
	padding: 0 4%;
	// border-right: 1px solid nth($grey, 1);

	svg, img{
		height: 18px;
		width: 24px;
		margin-top: 25%;
	}
}

.playon-ctrl{
	@include col(1);
	float: right;
	@include vertical-align;

	h3{
		font-size: 1.2em;
	}

	select#play-on{
		@include col(11);
		background: transparent;
		background: url(../images/icons/down-arrow-wht.svg) no-repeat 90%;
		border-radius: 2px;
		-webkit-appearance: none;
		-moz-appearance: none;
	    text-indent: 0.01px;
	    text-overflow: '';
		border: 1px solid nth($grey, 2);
		color: nth($grey, 3);
		font-size: .9em;
		padding: 3px;
		outline: none;
		height: 27px;
	}
}

















/*Dark Theme Overrides==============================================*/
.dark-fonts{
	color: #949494 !important;/*nth($grey, 3)*/
}

.dark-border-bottom{
	border-bottom: 1px solid #494F4F !important;/*nth($grey, 3)*/
}

.dark-border-right{
	border-right: 1px solid #494F4F !important;/*nth($grey, 3)*/
}

.dark-border-left{
	border-left: 1px solid #494F4F !important;/*nth($grey, 3)*/
}

.dark-placeholder::-webkit-input-placeholder { /* WebKit browsers */
    color: #494F4F !important;
}
.dark-placeholder:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: #494F4F !important;
}
.dark-placeholder::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: #494F4F !important;
}
.dark-placeholder:-ms-input-placeholder { /* Internet Explorer 10+ */
    color: #494F4F !important;
}

.dark-input-bg{
	background: #494F4F !important;
	border: none !important;
}



/* --END-- Dark Theme Overrides======================================*/

















