.search{
	overflow: hidden;
	@include vertical-align;
	position: relative;
	@include col(12);

	input{
		float: left;
		margin: 0;
		height: 48px;//39
		background: rgba(0,0,0,.1);/*slight dark overlay*/

		@include app-break-smmd{
			height: 72px
		}
	}

	input[type="text"]{
		@include col(10);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 1em;
		font-family: OpenSansLight;
		/*background: transparent;*/
		padding-left: 8.33333333%;
