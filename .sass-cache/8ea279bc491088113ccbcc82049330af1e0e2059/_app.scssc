section.app{
	@include col(9);
	float: left;
}

.section-header{
	height: 72px; //120
	border-bottom: 1px solid nth($grey, 4);
}

.search{
	overflow: hidden;
	@include vertical-align;
	position: relative;

	input{
		float: left;
		margin: 0;
		// padding: 0 6%;
		height: 72px;//39
	}

	input[type="text"]{
		@include col(11);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 2.7em;
		font-weight: 100;
		background: transparent;
		padding-left: 3%;
		outline: none;
	}

	input[type="submit"]{
		@include col(1);
		background: transparent;
		border-top-left-radius: 0;
		border-bottom-left-radius: 0;
		border-left: 1px solid nth($grey, 4);
	}

	input[type="submit"]:hover{
		opacity: .8;
	}

	input[type="text"]::-webkit-input-placeholder { /* WebKit browsers */
	    color: nth($grey, 4);
	}
	input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
	    color: nth($grey, 4);
	}
	input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
	    color: nth($grey, 4);
	}
	input[type="text"]:-ms-input-placeholder { /* Internet Explorer 10+ */
	    color: nth($grey, 4);
	}
}

#search-icon{
	position: absolute;
	right: 3.15%;
	top: 39%;
	height: 18px;
}

.main-container{
	ul{
		margin-bottom: 0;
		position: relative; /*required for submenu overflow visibility*/

		li{
			line-height: 48px;
			padding: 0 2%;
			overflow: hidden;

			svg,img,  .add-icon, .play-icon img{
				height: 12px;
				vertical-align: middle;
			}

			.add-icon{
				width: 12px;
				// margin-right: 31%;

			}

			.play-icon{
