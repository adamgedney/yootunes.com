html{
	background: $white;
}

//body#app
#app{
	background: $white;
	overflow: hidden;
	letter-spacing: 1px;/*give gothic font some space*/
}

aside.app{
	@include col(3);
	height: 100vh;
	float: left;
	border-right: 1px solid nth($grey, 4);
	overflow: hidden;
	// background: $black; /*experimenting with dark bg*/

	.video-size-ctrl{
		background: $black;
		@include col(3);
		//margin-left: 16.6666667%;
		position: fixed;
		bottom: 72px;
		left: 0;
		z-index:1002;
		height: 33px;
		text-align: right;
		border-bottom: 1px solid nth($grey, 1);

		svg, img{
			height: 27px;
			width: 27px;
			margin-left: 3px;
			cursor: pointer;
		}

		svg:first-child, img:first-child{
			width: 18px;
		}

		svg:last-child, img:last-child{
			width: 36px;
			margin-left: 0;
		}
	}
}

#collectionCount{
	font-size: .8em;
}

#video{
	// background: $black;//remove during tests. Placeholder bg color so video can be commented out
	@include col(3);
	position: fixed;
	bottom: 72px;
	left: 0;
	z-index:1000;
	height: 33px;
	border: none;
}

.loading{
	background: url('../images/icons/loading.GIF') no-repeat center;
	position: absolute;
	@include col(9);
	height: 60px;
	top: 45%;
	right: 0;
	display: none;
	opacity: .6;

	h2{
		margin-top: 63px;
		font-size: 1.5em;
		text-align: center;

	}
}







/*=====================================*/
/*Sidebar Settings=====================*/
/*=====================================*/
.library-nav{
	line-height: 36px;
	font-size: .8em;

	ul{
		margin-bottom: 0;

		li{
			padding: 0 6%;

			a{
				color: nth($grey, 2);
			}
		}
	}
}

#returnToSearch{
	margin-top: 27px;
