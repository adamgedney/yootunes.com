/*=====================================*/
/*Main Content container===============*/
/*=====================================*/
section.app{
	@include col(12);
	float: left;
	height: 100%;

	@include app-break-smmd{
		@include col(9);
	}
}



.section-header{
	height: 72px; //120
	border-bottom: 1px solid nth($grey, 4);
}

.search{
	overflow: hidden;
	@include vertical-align;
	position: relative;

	input{
		float: left;
		margin: 0;
		// padding: 0 6%;
		height: 72px;//39
	}

	input[type="text"]{
		@include col(10);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 1.2em;
		font-weight: 100;
		background: transparent;
		padding-left: 3%;
		outline: none;

		@include app-break-smmd{
			@include col(11);
			font-size: 2.7em;
		}

	}

	input[type="submit"]{
		@include col(2);
		background: transparent;
		border-top-left-radius: 0;
		border-bottom-left-radius: 0;
		border-left: 1px solid nth($grey, 4);

		@include app-break-smmd{
			@include col(1);
		}
	}

	input[type="submit"]:hover{
		opacity: .8;
	}

	input[type="text"]::-webkit-input-placeholder { /* WebKit browsers */
	    color: nth($grey, 4);
	}
	input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
	    color: nth($grey, 4);
	}
	input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
	    color: nth($grey, 4);
	}
	input[type="text"]:-ms-input-placeholder { /* Internet Explorer 10+ */
	    color: nth($grey, 4);
	}
}

