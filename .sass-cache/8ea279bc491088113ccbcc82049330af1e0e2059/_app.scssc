//Borowed from CSS-Tricks
.truncate {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

input[type="password"]{
	line-height: 16px;
}

/*--------------------------------------End Objects / Start Main------------------------------------*/

.no-select{
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: -moz-none;
    -o-user-select: none;
    user-select: none;
}

img, .section-header, aside.app, .li-header, .li-group, .footer{
	@extend .no-select;
}


