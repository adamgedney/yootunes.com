//footer popout for profile
.profile-edit-menu{
	width: 30%;
	height: auto;
	right: 1%;
	top: initial;
	bottom: 81px;
	padding: 1%;

	&:before{
		border-top: 9px solid  #fff;
		border-right: 3px solid transparent;
		border-left: 3px solid transparent;
		position: absolute;
		top: initial;
		bottom: -12px;
		right: 6.5%;

	}

	input{
		@include col(10);
		@include col-ctr(10);
		height: 45px;
	}

	input[type="submit"]{
		background: $red;
		color: $white;
	}

	form{
		margin-bottom: 72px;
	}

	a{
		text-decoration: underline;
		padding: 0 8.45% 9px;
	}

	a:nth-of-type(2){
		float: right;
	}
}

.profile-edit-menu-heading{
	margin: 36px 0 9px 8.45%;
}








/*=====================================*/
/*Footer Container=====================*/
/*=====================================*/
.footer{
	background: $black;
	height: 72px;
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	padding: 0 2%;
	color: $white;
	// overflow: hidden;

	-webkit-box-shadow: 0px -39px 34px -7px $white;
	   -moz-box-shadow: 0px -39px 34px -7px $white;
			box-shadow: 0px -39px 34px -7px $white;
}


.info{
	@include col(3);
	float: left;
	overflow: hidden;

	ul{
		float: left;
		margin: 9px 0 0 2%;

		li{
			font-size: .8em;
		}
	}

	img, svg{
		display: block;
		float: left;
		height: 63px;
		border-radius: 36px;
		margin-top: 4px;
	}
}

.transport-ctrl{
	@include col(1);
	float: left;
	overflow: hidden;
	height: 18px;
	@include vertical-align;

	svg, img{
		@include col(4);
		float: left;
		cursor: pointer;
	}

	.play{
		max-height: 17px;
		max-width: 17px;
		margin: 0 6px 0 9px;
	}

	.prev, .next{
		max-height: 12px;
		margin-top: 3px;
	}
}

.seek-ctrl{
	@include col(3);
	float: left;
	@include vertical-align;
	height: 36px;
	overflow: hidden;
	position: relative;

	p{
		font-size: 1.2em;
		display: inline;
		float: right;
		line-height: 0px; //important for lining up seek-line
	}
}

.seek-line{
	@include col(10);
	height: 0px;
	border-bottom: 1px solid nth($grey, 2);
	// @include vertical-align;
	margin-top:18px;
}

#seek-dot{
	width: 12px;
	height: 12px;
	background: $red;
	border-radius: 50%;
	position: absolute;
	top: 13px;
	z-index: 10;
	opacity: .9;
	cursor: pointer;
	border: 1px solid $black;
}

#seek-dot:active{
	width: 21px;
	height: 21px;
	top: 8px;
}

.volume-ctrl{
	@include col(1);
	float: left;
	height: 100%;
	padding-left: 1.5%;
	margin-top: 8px;

	.vol-icon{
		height: 24px;
		cursor: pointer;
		display: block;
		margin: 3px 0 0 27%;
	}

	.vol-icon:hover{
		opacity: .3;
	}
}

.playing-share-ctrl{
	@include col(1);
	float: left;
	@include vertical-align;
}

.playon-ctrl{
	@include col(1);
	float: right;
	@include vertical-align;

	h3{
		font-size: 1.2em;
	}

	select{
		@include col(11);
		background: transparent;
		background: url(../images/icons/down-arrow-wht.svg) no-repeat right;
		border-radius: 2px;
		-webkit-appearance: none;
		border: 1px solid $white;
		color: $white;
		font-size: .9em;
		padding: 3px;
		outline: none;
	}
}


.profile-display{
	@include col(2);
	float: right;
	overflow: hidden;
	height: 100%;
	padding: 0 3%;

	p{
		font-size: 1.2em;
		line-height: 72px;
		@include col(9);
		display: inline;
		float: right;
	}

	.settings-icon{
		float: right;
		height: 18px;
		@include col(3);
		@include vertical-align;
		cursor: pointer;
	}

	.settings-icon:hover{
		opacity: .7;
	}
}























