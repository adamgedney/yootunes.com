.info{
	@include col(3);
	float: left;
	overflow: hidden;
	// border-right: 1px solid nth($grey, 1);

	ul{
		float: left;
		margin: 9px 0 0 2%;

		li{
			font-size: .8em;
			overflow: hidden;
			font-size: .5em;
		}
