aside.app::-webkit-scrollbar {
    display: none;
}


.settings-menu a{
	padding-bottom: 306px;
}


.video-size-ctrl{
	background: $black;
	@include col(12);
	//margin-left: 16.6666667%;
	position: fixed;
	bottom: 72px;
	left: 0;
	z-index:990;
	height: 48px;
