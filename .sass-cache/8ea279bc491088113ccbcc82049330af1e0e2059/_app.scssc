.playlist-dropdown{
	background: #fff;
	display: none;//added to help jquery show/hide on load
}

li.playlist-dropdown:hover{
	background: #fff;
}

.playlist-popout{
	padding: 6% 0;

	p{
		font-size: 1em;
		margin: 0;
		overflow: hidden;
	}

	.share-strip{
		margin-bottom: 9px;
	}

	.bottom-inner{
		border-top: 1px solid nth($grey, 4);
		padding-top: 12px;
		cursor: pointer;
	}

	svg, img{
		cursor: pointer;
		height: 36px;
		width: 36px;
	}

	.trash-icon{
		height: 18px;
		width: 18px;
		float: right;
	}
}













/*=====================================*/
/*Main Content container===============*/
/*=====================================*/
section.app{
	@include col(9);
	float: left;
}

.section-header{
	height: 72px; //120
	border-bottom: 1px solid nth($grey, 4);
}

.search{
	overflow: hidden;
	@include vertical-align;

	input{
		float: left;
		margin: 0;
		// padding: 0 6%;
		height: 72px;//39
	}

	input[type="text"]{
		@include col(11);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 2.7em;
		font-weight: 100;
		background: transparent;
		padding-left: 3%;
		outline: none;
	}

	input[type="submit"]{
		@include col(1);
		background: $red;
		border-top-left-radius: 0;
		border-bottom-left-radius: 0;
		// border: 1px solid nth($grey, 3);
		border-left: none;
	}

	input[type="submit"]:hover{
		opacity: .8;
	}

	input[type="text"]::-webkit-input-placeholder { /* WebKit browsers */
	    color: nth($grey, 4);
	}
	input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
	    color: nth($grey, 4);
	}
	input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
	    color: nth($grey, 4);
	}
	input[type="text"]:-ms-input-placeholder { /* Internet Explorer 10+ */
	    color: nth($grey, 4);
	}
}

.main-container{
	ul{
		margin-bottom: 0;
		position: relative; /*required for submenu overflow visibility*/

		li{
			line-height: 48px;
			padding: 0 2%;
			overflow: hidden;

			svg,img,  .add-icon, .play-icon img{
				height: 12px;
				vertical-align: middle;
			}

			.add-icon{
				width: 12px;
				margin-right: 31%;
			}

			.play-icon{
				img{
					width: 10px;
					margin-left: 23%;
				}
			}
		}
	}
}

.scroll-container{
	height: 76vh;
	overflow: scroll;
}

/*Column widths & floats*/

.li-col1, .li-col2, .li-col3, .li-col4, .li-col5, .li-col6, .li-col7{
	max-height: 48px; //matches li line height
}

.li-col1{
	float: left;
}

.li-col1, .li-col7{
	width: 6%;
	display: inline-block;
}

.li-col2{
	display: inline-block;
	@include col(4);
	float: left;
}

.li-col3, .li-col4{
	@include col(2);
	display: inline-block;
	float: left;
	overflow: hidden;
}

.li-col6, .li-col5{
	@include col(1);
	float: right;
	display: inline-block;
}

.li-col6{
	min-width: 8.33333333%;
}

.li-col7{
	float: right;
	margin-top: 18px;

	svg, img{
		height: 12px;
		width: 12px;
		margin-right: 30%;
	}
}

.li-header{
	font-weight: 700;
	@include col(12);
	border-bottom: 1px solid nth($grey, 4);
	font-size: .8em;

	.li-col1{
		// height: 21px;
		line-height: 18px;
		font-size: .6em;
		padding-top: 19px;
		color: nth($grey, 3);

		svg, img{
			display: block;
		}
	}

	.li-col7{
		text-align: right;
		margin-top: 0;
	}

	.viewSongs, .viewArtists, .viewAlbums, .viewGenres{
		cursor: pointer;
	}
}

.li-group{
	color: nth($grey, 2);
	font-size: .8em;
	cursor: pointer;
}

li.li-group:nth-child(even){
	background: nth($grey, 6);
}













/*=====================================*/
/*li dropdown==========================*/
/*=====================================*/

li.li-group.dropdown{
	background: #fff;
	padding: 18px 18px 18px 9px;
	overflow: hidden;
	min-height: 240px;
	display: none;//Added to experiment with jquery show/hide on dropdowns

	.youtube-img{//youtube thumbnail
		width: 100%;
		height: 100%;
	}

	.li-desc{
		line-height: 18px;
		margin-left: 9px;
		display: inline-block;
		@include col(6);
	}
}

/*Right side menu in dropdown*/
ul.dropdown-main-menu{
	@include col(2);
	float: right;

	li{
		font-size: .7em;
		line-height: 27px;
	}

	li.li-share{
		margin: 36px 0 0 18px;

		svg, img{
			height: 21px;
		}
	}
}



/*=====================================*/
/*dropdown-main-menu-popouts===========*/
/*=====================================*/
ul.dropdown-main-menu{
	li{/*do not write styles here. Write them in the above ul.dropdown-main-menu li*/
		ul{
			width: 110%;
			position: absolute;
			right: 105%;
			height: 200px;
			overflow: auto;
			padding: 6% 0 0 6%;
			border-left: 1px solid nth($grey, 6);

			li{/*submenu level*/
				form{
					margin-bottom: 9px;
					margin-top: 0;
					padding-right: 12%;/*mimicks popout padding*/

					input{
						height: 24px;
						font-size: .6em;
						margin: 0;
						margin-bottom: 3px;
					}

					input[type="submit"]{
						line-height: 0px;
						background: $green;
						color: $white;
					}
				}
			}
		}
	}
}

ul.dropdown-main-menu li ul::-webkit-scrollbar {
    display: none;
}

.popout{
	font-size: .8em;
}

.add-to-playlist-menu{
	background: none;/*overrides popout styles*/
	box-shadow: none;
	padding-right: 0;/*orig: 6% Removed to allow right arrow to hit edge*/

	svg, img{/*more arrow right*/
		float: right;
	}

	p{
		margin-bottom: 15px;
	}

	p:last-child{
		margin-bottom: 3px;
	}

	li.playlist-menu-sub-list{
		font-size: 1.2em;
		line-height: 21px;
	}

	li.playlist-menu-sub-list:hover{
		background: nth($grey, 6);
	}
}


.playlist-menu-header{
	border-bottom: 1px solid nth($grey, 6);
	// margin-bottom: 3px;
}

.improve-meta-sub-menu{
	box-shadow: none;
}

.improve-meta-menu-form{

	p{
		margin-top: 27px;
		line-height: 9px;
		margin-bottom: 18px;
	}

	svg, img{
		float: right;
	}
}












//footer popout for profile
.profile-edit-menu{
	width: 30%;
	height: auto;
	right: 1%;
	top: initial;
	bottom: 81px;
	padding: 1%;

	&:before{
		border-top: 9px solid  #fff;
		border-right: 3px solid transparent;
		border-left: 3px solid transparent;
		position: absolute;
		top: initial;
		bottom: -12px;
		right: 6.5%;

	}

	input{
		@include col(10);
		@include col-ctr(10);
		height: 45px;
	}

	input[type="submit"]{
		background: $red;
		color: $white;
	}

	form{
		margin-bottom: 72px;
	}

	a{
		text-decoration: underline;
		padding: 0 8.45% 9px;
	}

	a:nth-of-type(2){
		float: right;
	}
}

.profile-edit-menu-heading{
	margin: 36px 0 9px 8.45%;
}








/*=====================================*/
/*Footer Container=====================*/
/*=====================================*/
.footer{
	background: $black;
	height: 72px;
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	padding: 0 2%;
	color: $white;
	// overflow: hidden;

	-webkit-box-shadow: 0px -60px 27px -7px rgba(235,235,235,.8);//$white
	   -moz-box-shadow: 0px -60px 27px -7px rgba(235,235,235,.8);
			box-shadow: 0px -60px 27px -7px rgba(235,235,235,.8);
}


.info{
	@include col(3);
	float: left;
	overflow: hidden;
	// border-right: 1px solid nth($grey, 1);

	ul{
		float: left;
		margin: 9px 0 0 2%;

		li{
			font-size: .8em;
			overflow: hidden;
		}
	}

	svg, img{
		display: block;
		float: left;
		height: 36px;
		width: 27px;
		margin: 18px 18px 0 0;
	}
}

.transport-ctrl{
	@include col(1);
	float: left;
	overflow: hidden;
	// height: 18px;
	height: 100%;
	// border-right: 1px solid nth($grey, 1);

	svg, img{
		@include col(4);
		float: left;
		cursor: pointer;
		@include vertical-align;
	}

	.play{
		max-height: 17px;
		max-width: 17px;
		margin: 0 6px 0 9px;
	}

	.prev, .next{
		max-height: 12px;
		margin-top: 1px;
	}
}

.seek-ctrl{
	@include col(3);
	float: left;
	@include vertical-align;
	height: 36px;
	overflow: hidden;
	position: relative;

	p{
		font-size: 1.2em;
		display: inline;
		float: right;
		line-height: 0px; //important for lining up seek-line
	}
}

.seek-line, .seek-fill{
	@include col(12);
	@include col-ctr(12);
	height: 0px;
	border-bottom: 1px solid nth($grey, 1);
	// @include vertical-align;
	margin-top:18px;
}

.seek-fill{
	width: 0;
	border-bottom: 1px solid $red;
	position: absolute;
}

#seek-dot{
	// width: 12px;
	// height: 12px;
	width: 0px;
	height: 0px;
	background: $black;
	border-radius: 50%;
	position: absolute;
	top: 13px;
	z-index: 10;
	opacity: .9;
	cursor: pointer;
	// border: 1px solid nth($grey, 2);
}

#seek-dot:active{
	width: 18px;
	height: 18px;
	top: 10px;
}

.volume-ctrl{
	@include col(1);
	float: left;
	height: 100%;
	// padding-right: 10%;
	// border-left: 1px solid nth($grey, 1);
	// border-right: 1px solid nth($grey, 1);

	.vol-icon{
		height: 24px;
		cursor: pointer;
		float: right;
		margin-left: 25%;
		@include vertical-align;
	}

	.vol-icon:hover{
		opacity: .3;
	}
}

.playing-share-ctrl{
	@include col(1);
	float: left;
	height: 100%;
	// border-right: 1px solid nth($grey, 1);

	svg, img{
		margin-top: 18%;
	}
}

.playon-ctrl{
	@include col(1);
	float: right;
	@include vertical-align;

	h3{
		font-size: 1.2em;
	}

	select{
		@include col(11);
		background: transparent;
		background: url(../images/icons/down-arrow-wht.svg) no-repeat right;
		border-radius: 2px;
		-webkit-appearance: none;
		border: 1px solid $white;
		color: $white;
		font-size: .9em;
		padding: 3px;
		outline: none;
	}
}

























