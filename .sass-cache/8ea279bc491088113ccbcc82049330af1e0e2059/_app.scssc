/*--------------------------------------End Objects / Start Main------------------------------------*/

.no-select{
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: -moz-none;
    -o-user-select: none;
    user-select: none;
}

img, .section-header, aside.app, .li-header, .footer{
	@extend .no-select;
}



html{
	background: $white;
}

//body#app
#app{
	background: $white;
	overflow: hidden;
	letter-spacing: 1px;/*give gothic font some space*/
}


#mobile-header{
	height: 48px;
	background: $black;
	position: relative;
	z-index: 9999;

	#menu-btn{
		display: inline-block;
		margin: 0 0 9px 18px;
	}

	.mobile-logo{
		display: inline-block;
		width: 155px;
		margin: 3px 22%;
	}

	@include app-break-smmd{
		display: none;
	}
}

// mobile slideout class
.sidebarSlideRight{
	left: 0 !important;
	@include duration;
}

.sidebarSlideLeft{
	left: -66.6666666% !important;
	@include duration;
}

aside.app{
	position: absolute;
	z-index: 998;
	/*display: none;*/
	margin-left: 0;
	left: -66.6666666%;
	@include col(8);
	min-height: 500px;
	height: 100vh;
	float: left;
	border-right: 1px solid nth($grey, 4);
	overflow: hidden;
	background: $black; /*experimenting with dark bg*/



	@include app-break-smmd{
		@include col(3);
		left: 0;
		display: block;
		position: relative;
		background: none;
	}
}

.video-size-ctrl{
	background: none;
	@include col(12);
	//margin-left: 16.6666667%;
	position: fixed;
	bottom: 72px;
	left: 0;
	z-index:990;
	height: 48px;
	text-align: right;
	border-bottom: 1px solid nth($grey, 1);

	svg, img{
		height: 24px;
		width: 27px;
		margin: 9px 0 0;
		padding-bottom: 11px;
		vertical-align: middle;
		cursor: pointer;
	}


	svg:first-child, img:first-child{
		width: 15px;
		height: 27px;
		margin-right: 3px;
	}

	svg:last-child, img:last-child{
		width: 36px;
		height: 27px;
		margin-left: -6px;
	}

	@include app-break-smmd{
		@include col(3);
		z-index: 1002;
		height: 33px;
		margin-top: 3px;
	}
}



#collectionCount{
