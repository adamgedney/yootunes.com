/*=====================================*/
/*li dropdown==========================*/
/*=====================================*/
/*li.li-group.dropdown{*/
.dropdown{
	/*background: #fff;*/
	padding: 18px 18px 18px 9px;
	overflow: hidden;
	min-height: 240px;
	display: none;//Added to experiment with jquery show/hide on dropdowns
	@include col(12);
	position: relative;

	.youtube-img{//youtube thumbnail
		width: 100%;
		/*height: 100%;*/
		min-height: 102px;
		max-height: 302px;
	}

	.li-desc{
		line-height:1.5;
		font-family: OpenSans;
		font-size: 1.5em;
		margin-left: 9px;
		display: inline-block;
		@include col(5);
	}

	.li-col1{
		@include col(2);
		max-height: 100%;
	}

}

/*Right side menu in dropdown*/
ul.dropdown-main-menu{
	@include col(2);
	float: right;

	li{
		font-size: .7em;
		line-height: 27px;
		overflow: visible;//Necessary for subplaylist options to show

		.addedToPlaylistCheck{
			height: 12px !important;//overrides main li specificity. Used in jquery affordance
		}
	}


	li.li-share{
		margin: 9px 0 0 24px;
		font-size: .6em;

		.share-strip{
			img.svg:first-child{
				padding-left: 0;
				width: 21px;
			}
		}

		svg, img{
			height: 21px;
			margin-right: 6px;
		}
	}

	li.li-buy{
		svg, img{
			height: 36px;
		}
		margin:0 0 0 18px;
	}
}



/*=====================================*/
/*dropdown-main-menu-popouts===========*/
/*=====================================*/
ul.dropdown-main-menu{
	li{/*do not write styles here. Write them in the above ul.dropdown-main-menu li*/
		ul{
			width: 130%;
			position: absolute;
			z-index: 10000;
			right: 105%;
			height: 200px;
			overflow: auto;
			padding: 0 0 0 6%;
			border-left: 1px solid nth($grey, 6);

			li{/*submenu level*/
				form{
					margin-bottom: 9px;
					margin-top: 0;
					padding-right: 12%;/*mimicks popout padding*/

					input{
						height: 24px;
						font-size: 1.5em;
						margin: 0;
						margin-bottom: 3px;
					}

					input[type="submit"]{
						line-height: 0px;
						background: $green;
						color: $white;
					}
				}
			}
		}
	}
}

ul.dropdown-main-menu li ul::-webkit-scrollbar {
    display: none;
}


#bg-container{
	position: absolute;
}

.popout{
	font-size: .8em;
}

.add-to-playlist-menu{
	background: none;/*overrides popout styles*/
	box-shadow: none;
	padding-right: 0;/*orig: 6% Removed to allow right arrow to hit edge*/

	svg, img{/*more arrow right*/
		float: right;
	}

	p{
		margin-bottom: 15px;
	}

	p:last-child{
		margin-bottom: 3px;
	}

	li.playlist-menu-sub-list{
		font-size: 1.2em;
		line-height: 21px;
	}

	li.playlist-menu-sub-list:hover{
		background: nth($grey, 6);
	}
}

.add-to-playlist-menu-trigger{
	img{
		height: 9px !important; //overrides the add to playlist link icon size set by main-container li
	}
}

 .addedToPlaylistCheck{
 	display: none;
 }


.playlist-menu-header{
	border-bottom: 1px solid nth($grey, 6);
	// margin-bottom: 3px;
}






