ul.dropdown-main-menu{
	@include col(2);
	float: right;

	li{
		font-size: .7em;
		line-height: 27px;

		.addedToPlaylistCheck{
			height: 12px !important;//overrides main li specificity. Used in jquery affordance
		}
	}

	li.li-share{
		margin: 9px 0 0 24px;
		font-size: .6em;

		.share-strip{
			img.svg:first-child{
