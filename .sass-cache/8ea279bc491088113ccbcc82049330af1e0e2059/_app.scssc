html{
	background: $white;
}

//body#app
#app{
	background: $white;
	overflow: hidden;
}

aside.app{
	@include col(3);
	height: 100vh;
	float: left;
	border-right: 1px solid nth($grey, 4);
	overflow: hidden;
	// background: $black; /*experimenting with dark bg*/

	.video-size-ctrl{
		background: $black;
		@include col(3);
		//margin-left: 16.6666667%;
		position: fixed;
		bottom: 72px;
		left: 0;
		z-index:1002;
		height: 33px;
		text-align: right;
		border-bottom: 1px solid nth($grey, 1);

		svg, img{
			height: 27px;
			width: 27px;
			margin-left: 3px;
			cursor: pointer;
		}

		svg:first-child, img:first-child{
			width: 18px;
		}

		svg:last-child, img:last-child{
			width: 36px;
			margin-left: 0;
		}
	}
}

#video{
	// background: $black;//remove during tests. Placeholder bg color so video can be commented out
	@include col(3);
	position: fixed;
	bottom: 72px;
	left: 0;
	z-index:1000;
	height: 33px;
	border: none;
}

.loading{
	background: url('../images/icons/loading.GIF') no-repeat center;
	position: absolute;
	@include col(9);
	height: 60px;
	top: 45%;
	right: 0;
	display: none;
	opacity: .6;

	h2{
		margin-top: 63px;
		font-size: 1.5em;
		text-align: center;

	}
}







/*=====================================*/
/*Sidebar Settings=====================*/
/*=====================================*/
.library-nav{
	line-height: 36px;
	font-size: .8em;

	ul{
		margin-bottom: 0;

		li{
			padding: 0 6%;

			a{
				color: nth($grey, 2);
			}
		}
	}
}

#returnToSearch{
	margin-top: 27px;
	vertical-align: top;
	line-height: 18px;
	overflow: hidden;
	cursor: pointer;

	img{
		height: 18px;
		float: right;
	}
}

#returnToSearch:hover{
	background: none;
}


.settings-menu{
	margin-top: 33px;
}

.library-menu{
	margin-top: 18px;
	@include col(12);
}

#collectionCount{
	float: right;
}






/*=====================================*/
/*Sidebar playlist=====================*/
/*=====================================*/
.playlist-nav{
	font-size: .8em;
	margin-top: 9px;

	h3{
		display: inline;
		line-height: 36px;
		padding: 0 6%;
	}

	ul{
		li{
			line-height: 24px;
			cursor: pointer;
			padding: 0 6%;
			position: relative;

			a{
				color: nth($grey, 2);
				@include col(11);
			}

		}
	}
}

.playlist-menu{
	@include col(1);
	float: right;

	svg, img{
		height: 12px;
		vertical-align: middle;
	}
}

.add-icon{
	float: right;
}

.playlist-scroll-container::-webkit-scrollbar {
    display: none;
}

.playlist-scroll-container{
	height: 47vh;
	overflow: scroll;
}






/*=====================================*/
/*Base popout & Playlist popout========*/
/*=====================================*/
.popout{
	position: absolute;
	right: 14.5%;
	top: 0px;
}

.playlist-dropdown{
	background: #fff;
	display: none;//added to help jquery show/hide on load
}

li.playlist-dropdown:hover{
	background: #fff;
}

.playlist-popout{
	padding: 6% 0;

	p{
		font-size: 1em;
		margin: 0;
		overflow: hidden;
	}

	.share-strip{
		margin-bottom: 9px;
	}

	.bottom-inner{
		border-top: 1px solid nth($grey, 4);
		padding-top: 12px;
		cursor: pointer;
	}

	svg, img{
		cursor: pointer;
		height: 36px;
		width: 36px;
	}

	.trash-icon{
		height: 18px;
		width: 18px;
		float: right;
	}
}













/*=====================================*/
/*Main Content container===============*/
/*=====================================*/
section.app{
	@include col(9);
	float: left;
}

.section-header{
	height: 72px; //120
	border-bottom: 1px solid nth($grey, 4);
}

.search{
	overflow: hidden;
	@include vertical-align;
	position: relative;

	input{
		float: left;
		margin: 0;
		// padding: 0 6%;
		height: 72px;//39
	}

	input[type="text"]{
		@include col(11);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 2.7em;
		font-weight: 100;
		background: transparent;
		padding-left: 3%;
		outline: none;
	}

	input[type="submit"]{
		@include col(1);
		background: transparent;
		border-top-left-radius: 0;
		border-bottom-left-radius: 0;
		border-left: 1px solid nth($grey, 4);
	}

	input[type="submit"]:hover{
		opacity: .8;
	}

	input[type="text"]::-webkit-input-placeholder { /* WebKit browsers */
	    color: nth($grey, 4);
	}
	input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
	    color: nth($grey, 4);
	}
	input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
	    color: nth($grey, 4);
	}
	input[type="text"]:-ms-input-placeholder { /* Internet Explorer 10+ */
	    color: nth($grey, 4);
	}
}

#search-icon{
	position: absolute;
	right: 3.15%;
	top: 39%;
	height: 18px;
}

.main-container{
	ul{
		margin-bottom: 0;
		position: relative; /*required for submenu overflow visibility*/

		li{
			line-height: 48px;
			padding: 0 2%;
			overflow: hidden;

			svg,img,  .add-icon, .play-icon img{
				height: 12px;
				vertical-align: middle;
			}

			.add-icon{
				width: 12px;
				// margin-right: 31%;

			}

			.play-icon{
				img{
					width: 10px;
					margin-left: 23%;
				}
			}

			.createNewPlaylistLink, .addToPlaylistTitle{
				font-size: 1.5em;
				line-height: 9px;

				img{
					float: left;
					margin: 0px 6px 0 0;
					height: 9px;
				}
			}
		}
	}
}

.addToPlaylistTitle{
	padding-bottom: 18px;

	img.more-arrow-down{
		width: 9px;
	}
}

