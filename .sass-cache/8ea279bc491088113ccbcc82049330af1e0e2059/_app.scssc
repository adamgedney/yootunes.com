.share-strip{
	overflow: hidden;

	svg, img{
		@include col(3);
		display: block;
		float: left;
		padding-left: 6px;
	}
}


.li-group:hover, .li-playlist:hover, .library-nav li:hover{
	background: nth($grey, 5);
}


svg:hover, img:hover{
	opacity: .8;
