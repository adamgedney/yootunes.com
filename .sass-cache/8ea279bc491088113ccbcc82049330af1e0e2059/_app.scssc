/*--------------------------------------End Objects / Start Main------------------------------------*/

.no-select{
    -webkit-user-select: none !important;
    -khtml-user-select: none !important;
    -moz-user-select: -moz-none !important;
    -o-user-select: none !important;
    user-select: none !important;
}

/* *:not(input.field) */
li, .footer, span, .section-header, .search, #video, #adsense, .video-size-ctrl,  img, #searchForm input, section.app, aside.app  {
   @extend .no-select;
}



html{
	background: $dark-bg;
}

#app{
	color: nth($grey, 3);
	background: $dark-bg;
	overflow: hidden;
	letter-spacing: 1px;/*give gothic font some space*/
}


#mobile-header{
	height: 48px;
	background: $black;
	position: relative;
	z-index: 9999;

	#menu-btn{
		display: inline-block;
		margin: 0 0 3px 18px;
		border-radius: 2px;
		border: 1px solid nth($grey, 1);
		padding: 6px;
	}

	.mobile-logo{
		display: inline-block;
		width: 125px;
		margin: 3px 0 3px 20%;

		@include app-break-sm{
			margin: 3px  0 3px 30%;
			width: 155px;
		}

		@include app-break-smmd{
			margin: 3px 0 3px 42%;
		}
	}

	@include app-break-smmd{
		display: none;
	}
}

#minimizeOverlay{
	position: absolute;
	top: 13px;
	right: 9px;
	// height: 48px;
	width: 40px;
	// @include col(1);

	img{
		@include col(10);
	}
}


// mobile slideout class
.sidebarSlideRight{
	left: 0 !important;
	@include duration;
}

.sidebarSlideLeft{
	left: -66.6666666% !important;
	@include duration;
}

aside{
	position: absolute;
	z-index: 998;
	/*display: none;*/
	margin-left: 0;
	left: -66.6666666%;
	@include col(8);
	min-height: 500px;
	height: 100vh;
	float: left;
	border-right: 1px solid nth($grey, 1);
	overflow: scroll;
	padding-bottom: 306px;
	background: $black; /*experimenting with dark bg*/
	color: nth($grey, 3);

	li{
		line-height: 42px;
		// border-bottom: 1px solid nth($grey, 1);

		@include app-break-smmd{
			line-height: 27px;
			border-bottom: none;
		}
	}

	li:first-child{
		border-bottom: none;
	}

	@include app-break-smmd{
		@include col(3);
		left: 0;
		display: block;
		position: relative;
		background: none;
	}
}



aside.app::-webkit-scrollbar {
    display: none;
}


.settings-menu a{
	padding-bottom: 306px;
}


.video-size-ctrl{
	background: $black;
	@include col(12);
	//margin-left: 16.6666667%;
	position: fixed;
	bottom: 72px;
	left: 0;
	z-index:990;
	height: 48px;
	text-align: right;
	border-bottom: 1px solid nth($grey, 1);

	svg, img{
		height: 24px;
		width: 27px;
		margin: 9px 0 0;
		padding-bottom: 11px;
		vertical-align: middle;
		cursor: pointer;
	}


	svg:first-child, img:first-child{
		width: 15px;
		height: 27px;
		margin-right: 3px;
	}

	svg:last-child, img:last-child{
		width: 39px;
		height: 30px;
		margin-left: -6px;
	}

	@include app-break-smmd{
		@include col(3);
		z-index: 1002;
		height: 33px;
		margin-top: 3px;
	}
}



#collectionCount{
	font-size: .8em;
}

#video, #video-overlay{
	background: $black;
	@include col(12);
	position: fixed;
	bottom: 87px;
	left: 0;
	/*z-index:1005;*/
	z-index: 990;
	height: 33px;
	border: none;

	@include app-break-smmd{
		@include col(3);
		z-index:1000;
		bottom: 72px;
	}
}

#video-overlay{
	bottom: 0px;

	@include app-break-smmd{
		display: none;
	}
}



.loading{
	background: url('../images/icons/loading.GIF') no-repeat center;
	position: absolute;
	@include col(9);
	height: 60px;
	top: 45%;
	right: 0;
	display: none;
	opacity: .6;

	h2{
		margin-top: 63px;
		font-size: 1.5em;
		text-align: center;

	}
}







