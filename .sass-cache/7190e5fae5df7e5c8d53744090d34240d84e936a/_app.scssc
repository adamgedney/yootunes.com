/*--------------------------------------End Objects / Start Main------------------------------------*/

.no-select{
    -webkit-user-select: none !important;
    -khtml-user-select: none !important;
    -moz-user-select: -moz-none !important;
    -o-user-select: none !important;
    user-select: none !important;
}

/* *:not(input.field) */
li, .footer, span, .section-header, .search, #video, #adsense, .video-size-ctrl,  img, #searchForm input, section.app, aside.app  {
   @extend .no-select;
}



html{
	background: $dark-bg;
}

#app{
	color: nth($grey, 3);
	background: $dark-bg;
	overflow: hidden;
	letter-spacing: 1px;/*give gothic font some space*/
}


#mobile-header{
	height: 48px;
	background: $black;
	position: relative;
	z-index: 9999;

	#menu-btn{
