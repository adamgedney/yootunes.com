/*Column widths & floats*/

.li-col1, .li-col2, .li-col3, .li-col4, .li-col5, .li-col6, .li-col7{
	max-height: 48px; //matches li line height
	@extend .truncate;
}

.li-col1{
	float: left;
}

.li-col1, .li-col7{
	width: 6%;
	display: inline-block;
}

.li-col2{
	display: inline-block;
	@include col(4);
	float: left;
	padding-right: 2%;
	margin-right: 2%;
}

.li-col3, .li-col4{
	@include col(2);
	display: inline-block;
	float: left;
}

.li-col3{
	width: 200px;//Testing float fix where content is empty
}

.li-col3:before{
	content: " ";
	width: 200px;
}

.li-col6, .li-col5{
	@include col(1);
	float: right;
	display: inline-block;
}

.li-col6{
	min-width: 8.33333333%;
}

.li-col7{
	float: right;
	margin-top: 18px;

	svg, img{
		height: 12px;
		width: 12px;
		margin-right: 30%;
	}
}

.li-header{
	font-weight: 700;
	@include col(12);
	border-bottom: 1px solid nth($grey, 4);
	font-size: .8em;

	.li-col1{
		// height: 21px;
		line-height: 18px;
		font-size: .6em;
		padding-top: 19px;
		color: nth($grey, 3);

		svg, img{
			display: block;
			margin-left: 24%;
		}
	}

	.li-col7{
		text-align: right;
		margin-top: 0;
	}

	.viewSongs, .viewArtists, .viewAlbums, .viewGenres{
		cursor: pointer;
	}
}

.li-group{
	color: nth($grey, 2);
	font-size: .8em;
	cursor: pointer;
}

li.li-group:nth-child(even){
	background: nth($grey, 6);
}













