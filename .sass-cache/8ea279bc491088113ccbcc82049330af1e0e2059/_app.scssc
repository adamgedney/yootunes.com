/*==================================*/
/*Account Settings Page*/
/*==================================*/

.info-wrapper{
	overflow: hidden;
	padding: 6% 6% 12%;
	position: relative;
	min-height: 600px;


	input[type="text"], input[type="password"], input[type="submit"], select, button{
		height: 36px;
	}

	input[type="submit"], button{
		@include col(12);
		height: 45px;
		background: $blue;
		color: $white;
		line-height: 16px;
		border: none;
	}

	label{
		display: block;
		font-size: 1.2em;
		color: nth($grey, 2);
	}

