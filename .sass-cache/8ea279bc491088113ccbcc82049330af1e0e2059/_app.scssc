}

.sidebarSlideLeft{
	left: -66.6666666% !important;
	@include duration;
}

aside{
	position: absolute;
	z-index: 998;
	/*display: none;*/
	margin-left: 0;
	left: -66.6666666%;
	@include col(8);
	min-height: 500px;
	height: 100vh;
	float: left;
	border-right: 1px solid nth($grey, 1);
	overflow: scroll;
	padding-bottom: 306px;
	background: $black; /*experimenting with dark bg*/
	color: nth($grey, 3);

	li{
		line-height: 42px;
		border-bottom: 1px solid nth($grey, 1);

		@include app-break-smmd{
			line-height: 27px;
			border-bottom: none;
		}
	}

	li:first-child{
		border-bottom: none;
	}

