	padding-right: 2%;
	margin-right: 2%;

	@include app-break-smmd{
		@include col(4);
	}
}

.li-col3, .li-col4{
	@include col(2);
	display: inline-block;
	float: left;
	display: none;

	@include app-break-smmd{
