/*=====================================*/
/*Main Content container===============*/
/*=====================================*/
// mobile slideout class
.mainSlideRight{
	margin-left: 66.6666666%;
	@include duration;
}

.mainSlideLeft{
	margin-left: 0;
	@include duration;
}

section.app{
	@include col(12);
	float: left;
	height: 100%;

	@include app-break-smmd{
		@include col(9);
	}
}



.section-header{
	height: 48px; //120
	border-bottom: 1px solid nth($grey, 1);

	@include app-break-smmd{
		height: 72px
	}
}

.search{
	overflow: hidden;
	@include vertical-align;
	position: relative;
	@include col(12);

	input{
		float: left;
		margin: 0;
		height: 48px;//39
		background: rgba(0,0,0,.1);/*slight dark overlay*/

		@include app-break-smmd{
			height: 72px;
			line-height: 72px;
		}
	}

	input[type="text"]{
		@include col(10);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 1em;
		font-family: OpenSansLight;
		/*background: transparent;*/
		padding-left: 8.33333333%;
		outline: none;
		color: nth($grey, 4);


		@include app-break-smmd{
			@include col(11);
			font-size: 2.7em;
		}

	}

	input[type="submit"]{
		@include col(2);
		/*background: transparent;*/
		border-top-left-radius: 0;
		border-bottom-left-radius: 0;
		border-left: 1px solid nth($grey, 1);

		@include app-break-smmd{
			@include col(1);
		}
	}

	input[type="submit"]:hover{
		opacity: .8;
	}

	input[type="text"]::-webkit-input-placeholder { /* WebKit browsers */
		font-family: OpenSansLight;
	    color: nth($grey, 3);
	    font-size: 1.2em;

	    @include app-break-smmd{
			font-size: 1em;
		}
	}
	input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
		font-family: OpenSansLight;
	    color: nth($grey, 3);

	    @include app-break-smmd{
			font-size: 1em;
		}
	}
	input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
		font-family: OpenSansLight;
	    color: nth($grey, 3);

	    @include app-break-smmd{
			font-size: 1em;
		}
	}
	input[type="text"]:-ms-input-placeholder { /* Internet Explorer 10+ */
		font-family: OpenSansLight;
	    color: nth($grey, 3);

	    @include app-break-smmd{
			font-size: 1em;
		}
	}

}

#returnToSearch{
	position: absolute;
	margin: 14px 0 0 2%;
	z-index: 1;
	cursor: pointer;

	img{
		height: 15px;

		@include app-break-smmd{
			height: 18px;
		}
	}

	@include app-break-smmd{
		margin: 24px 0 0 2%;
	}
}

#returnToSearch:hover{
	background: none;
}

#datalistContainer{
	@include col(12);
	background: nth($grey, 5);
	position: absolute;
	top: 96px;
	height: auto;
	z-index: 15000;

		li{
			line-height: 27px;
		}

		li:first-child{
			color: nth($grey, 1);
			border-bottom: 1px solid nth($grey, 4);
		}

		li:hover{
			background: rgba(0,0,0,.1);
			cursor: pointer;
		}

		li:first-child:hover{
			background: none;
		}

	@include app-break-smmd{
		@include col(9);
		top: 68px;
	}
}

#search-icon{
	position: absolute;
	right: 6.15%;
	top: 39%;
	height: 18px;

	@include app-break-smmd{
		right: 3.15%;
	}
}

.main-container{
	ul{
		margin-bottom: 0;
		position: relative; /*required for submenu overflow visibility*/

		li{
			line-height: 48px;
			padding: 0 2%;
			overflow: hidden;
			color: nth($grey, 3);
			position: relative;

			svg,img,  .add-icon, .play-icon img{
				height: 12px;
				vertical-align: middle;
			}

			.draggableIcon{
				position: absolute;
				top: 11px;
				left: 6px;
				height: 27px;
				opacity: .3;
				display: none;
			}

			.add-icon{
				width: 12px;
				// margin-right: 31%;

			}

			.play-icon{
				img{
					width: 15px;
					height: 15px;
					margin-left: 23%;
					position: relative;
					z-index: 1;
				}
			}

			.createNewPlaylistLink, .addToPlaylistTitle{
				font-size: 1.5em;
				line-height: 9px;

				img{
					float: left;
					margin: 0px 6px 0 0;
					height: 9px;
				}
			}
		}
	}
}

.newPlaylistForm{
	display: none; //used in show/hide
}



.addToPlaylistTitle{
	padding-bottom: 18px;

	img.more-arrow-down{
		width: 9px;
	}
}



#scroll-container{
	height: 74vh;
	min-height: 500px;
	overflow: scroll;
}

/*Column widths & floats*/

.li-col1, .li-col2, .li-col3, .li-col4, .li-col5, .li-col6, .li-col7{
	max-height: 48px; //matches li line height
	@extend .truncate;
}

.li-col1{
	float: left;
}

.li-col1, .li-col7{
	@include col(1);

	@include app-break-smmd{
		width: 6%;
	}
}

.li-col2{
	display: inline-block;
	@include col(10);
	float: left;
	padding-right: 2%;
	margin-right: 2%;

	@include app-break-smmd{
		@include col(5);
	}
}

.li-col3, .li-col4{
	@include col(2);
	display: inline-block;
	float: left;
	display: none;

	@include app-break-smmd{
		display: block;
	}
}

.li-col3{

}

/*.li-col3:before{
	content: " ";
	width: 200px;
}*/

.li-col6, .li-col5{
	@include col(1);
	float: right;
	display: none;

	@include app-break-smmd{
		display: inline-block;
	}
}

.li-col6{
	min-width: 8.33333333%;
}

.li-col7{
	float: right;
	margin-top: 18px;
	/*display: none;*/

	svg, img{
		height: 12px;
		width: 12px;
		margin-right: 30%;
	}

	@include app-break-smmd{
		/*display: inline-block;*/
	}
}

.li-header{
	height: 27px;
	/*line-height: 27px;*/
	font-weight: 700;
	@include col(12);
	border-bottom: 1px solid nth($grey, 1);
	font-size: .6em;
	background: rgba(0,0,0,.2);

	/*-webkit-box-shadow: 0px 6px 10px 0px rgba(0, 0, 0, 0.2);
	   -moz-box-shadow: 0px 6px 10px 0px rgba(0, 0, 0, 0.2);
			box-shadow: 0px 6px 10px 0px rgba(0, 0, 0, 0.2);*/

	.li-col1, .li-col2, .li-col3, .li-col4, .li-col5, .li-col6, .li-col7{
		max-height: 27px;
		line-height: 27px;
		font-family: OpenSansLight;
	}


	.li-col1{
		font-size: .6em;
		padding-top: 19px;
		color: nth($grey, 3);

		svg, img{
			display: block;
			margin-left: 24%;
		}
	}

	.li-col5{
		@include col(2);
		padding-left: 2%;
	}

	.li-col7{
		text-align: right;
		margin-top: 0;
		display: none;

		@include app-break-smmd{
			display: inline-block;		}
	}

	.viewSongs, .viewArtists, .viewAlbums, .viewGenres, .mainViewSongs, .mainViewArtists, .mainViewAlbums, .mainViewGenres{
		cursor: pointer;
	}

	@include app-break-smmd{
		font-size: .8em;
	}
}

.sortIcon{
	width: 8px;
	opacity: .1;
	margin: 6px 0 0 3px;
}

.li-group{
	color: nth($grey, 2);
	font-size: .8em;
	cursor: pointer;
}

