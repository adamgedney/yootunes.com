/*=====================================*/
/*Base popout & Playlist popout========*/
/*=====================================*/
.popout{
	position: absolute;
	right: 14.5%;
	top: 0px;
}

.playlist-dropdown{
	background: #fff;
	display: none;//added to help jquery show/hide on load
}

li.playlist-dropdown:hover{
	background: #fff;
}

.playlist-popout{
	padding: 6% 0;

	p{
		font-size: 1em;
		margin: 0;
		overflow: hidden;
		color: nth($grey, 2);
	}

	.share-strip{
		margin-bottom: 9px;

		img{
			width: 32px;
			height: 32px;
			margin-right: 6px;

			@include app-break-smmd{
				width: 21px;
				height: 21px;
			}
		}

		img:first-child{
			padding-left: 0;
			margin-left: 0;
		}
	}

	.bottom-inner{
		border-top: 1px solid nth($grey, 4);
		padding-top: 12px;
		cursor: pointer;
	}

	svg, img{
		cursor: pointer;
		height: 36px;
		width: 36px;
	}

	.trash-icon{
		height: 15px;
		width: 15px;
		float: right;
	}
}













/*=====================================*/
/*Main Content container===============*/
/*=====================================*/
// mobile slideout class
.mainSlideRight{
	margin-left: 66.6666666%;
	@include duration;
}

.mainSlideLeft{
	margin-left: 0;
	@include duration;
}

section.app{
	@include col(12);
	float: left;
	height: 100%;

	@include app-break-smmd{
		@include col(9);
	}
}



.section-header{
	height: 48px; //120
	border-bottom: 1px solid nth($grey, 1);

	@include app-break-smmd{
		height: 72px
	}
}

.search{
	overflow: hidden;
	@include vertical-align;
	position: relative;
	@include col(12);

	input{
		float: left;
		margin: 0;
		height: 48px;//39
		background: rgba(0,0,0,.1);/*slight dark overlay*/

		@include app-break-smmd{
			height: 72px
		}
	}

	input[type="text"]{
		@include col(10);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 1em;
		font-family: OpenSansLight;
		/*background: transparent;*/
		padding-left: 8.33333333%;
		outline: none;
		color: nth($grey, 4);


		@include app-break-smmd{
			@include col(11);
			font-size: 2.7em;
		}

	}

	input[type="submit"]{
		@include col(2);
		/*background: transparent;*/
		border-top-left-radius: 0;
		border-bottom-left-radius: 0;
		border-left: 1px solid nth($grey, 1);

		@include app-break-smmd{
			@include col(1);
		}
	}

	input[type="submit"]:hover{
		opacity: .8;
	}

	input[type="text"]::-webkit-input-placeholder { /* WebKit browsers */
		font-family: OpenSansLight;
	    color: nth($grey, 3);
	    font-size: 1.2em;

	    @include app-break-smmd{
			font-size: 1em;
		}
	}
	input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
		font-family: OpenSansLight;
	    color: nth($grey, 3);

	    @include app-break-smmd{
			font-size: 1em;
		}
	}
	input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
		font-family: OpenSansLight;
	    color: nth($grey, 3);

	    @include app-break-smmd{
			font-size: 1em;
		}
	}
	input[type="text"]:-ms-input-placeholder { /* Internet Explorer 10+ */
		font-family: OpenSansLight;
	    color: nth($grey, 3);

	    @include app-break-smmd{
			font-size: 1em;
		}
	}

}

