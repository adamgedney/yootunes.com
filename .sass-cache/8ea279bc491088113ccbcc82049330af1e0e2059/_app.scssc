	@include app-break-smmd{
		display: inline-block;
	}
}

.li-header{
	font-weight: 700;
	@include col(12);
	border-bottom: 1px solid nth($grey, 4);
	font-size: .6em;

	.li-col1{
		// height: 21px;
		line-height: 18px;
		font-size: .6em;
		padding-top: 19px;
		color: nth($grey, 3);

		svg, img{
			display: block;
			margin-left: 24%;
		}
	}

	.li-col7{
		text-align: right;
