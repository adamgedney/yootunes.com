// }


.share-strip{
	overflow: hidden;

	svg{
		@include col(3);
		display: block;
		float: left;
		padding-left: 6px;
	}
}


.li-group:hover, .playlist-nav li:hover, .library-nav li:hover{
	background: nth($grey, 5);
}


/*--------------------------------------End Objects / Start Main------------------------------------*/


//body#app
#app{
	background: $white;
	overflow: hidden;
}

aside.app{
	@include col(3);
	height: 100vh;
	float: left;
	border-right: 1px solid nth($grey, 4);
	overflow: hidden;
	// background: $black; /*experimenting with dark bg*/

	iframe{
		background: $black;//remove during tests. Placeholder bg color so video can be commented out
		@include col(3);
		position: fixed;
		bottom: 72px;
		left: 0;
		z-index:1000;
		height: 27px;
		border: none;
	}

	.video-size-ctrl{
		background: $black;
		opacity: .9;
		@include col(3);
		//margin-left: 16.6666667%;
		position: fixed;
		bottom: 72px;
		left: 0;
		z-index:1001;
		height: 27px;
		text-align: right;

		svg{
			height: 27px;
			width: 27px;
			margin-left: 3px;
			cursor: pointer;
		}

		svg:first-child{
			width: 18px;
		}

		svg:last-child{
			width: 36px;
			margin-left: 0;
		}
	}
}



/*=====================================*/
/*Sidebar Settings=====================*/
/*=====================================*/
.library-nav{
	line-height: 36px;
	font-size: .8em;

	ul{
		li{
			padding: 0 6%;

			a{
				color: nth($grey, 2);
			}
		}
	}
}

.settings-menu{
	margin-top: 72px;
}

.library-menu{
	margin-top: 18px;
}




/*=====================================*/
/*Sidebar playlist=====================*/
/*=====================================*/
.playlist-nav{
	font-size: .8em;

	h3{
		display: inline;
		line-height: 36px;
		padding: 0 6%;
	}

	ul{
		li{
			line-height: 42px;
			cursor: pointer;
			padding: 0 6%;
			position: relative;

			a{
				color: nth($grey, 2);
				@include col(11);
			}

		}
	}
}

.playlist-menu{
	@include col(1);
	float: right;

	svg{
		height: 12px;
		vertical-align: middle;
	}
}

.add-icon{
	float: right;
}

.playlist-scroll-container::-webkit-scrollbar {
    display: none;
}

.playlist-scroll-container{
	height: 65.25vh;
	overflow: scroll;

-webkit-box-shadow: inset 0px -31px 24px -7px $white;
	   -moz-box-shadow: inset 0px -31px 24px -7px $white;
			box-shadow: inset 0px -31px 24px -7px $white;
}






/*=====================================*/
/*Base popout & Playlist popout========*/
/*=====================================*/

.popout{
	width: 41%;
	height: 111px;
	background: #fff;
	position: absolute;
	right: 14.5%;
	top: 0px;
	border-radius: 2px;
	padding: 6%;
	z-index: 100;

	-webkit-box-shadow: 7px 7px 11px -5px rgba(24, 26, 26, 0.05);
	   -moz-box-shadow: 7px 7px 11px -5px rgba(24, 26, 26, 0.05);
			box-shadow: 7px 7px 11px -5px rgba(24, 26, 26, 0.05);

	&:before{
		content:"";
		width: 0;
		height: 0;
		border-top: 3px solid transparent;
		border-bottom: 3px solid transparent;
		border-left: 9px solid #fff;
		position: absolute;
		top: 18px;
		right: -9px;
		z-index: 99;
	}

}

.playlist-popout{

	p{
		font-size: .8em;
		margin: 0;
		overflow: hidden;
	}

	.share-strip{
		margin-bottom: 9px;
	}

	.bottom-inner{
		border-top: 1px solid nth($grey, 4);
		padding-top: 12px;
		cursor: pointer;
	}

	svg{
		cursor: pointer;
		height: 18px;
		width: 18px;
	}

	.trash-icon{
		height: 12px;
		width: 12px;
		float: right;
	}
}













/*=====================================*/
/*Main Content container===============*/
/*=====================================*/
section.app{
	@include col(9);
	float: left;
}

.section-header{
	height: 72px; //120
	border-bottom: 1px solid nth($grey, 4);
}

.search{
	overflow: hidden;
	@include vertical-align;

	input{
		float: left;
		margin: 0;
		// padding: 0 6%;
		height: 72px;//39
	}

	input[type="text"]{
		@include col(11);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 3.6em;
		font-weight: 100;
		background: transparent;
		padding-left: 3%;
		outline: none;
	}

	input[type="submit"]{
		@include col(1);
		background: $red;
		border-top-left-radius: 0;
		border-bottom-left-radius: 0;
		// border: 1px solid nth($grey, 3);
		border-left: none;
	}

	input[type="submit"]:hover{
		opacity: .8;
	}

	input[type="text"]::-webkit-input-placeholder { /* WebKit browsers */
	    color: nth($grey, 5);
	}
	input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
	    color: nth($grey, 5);
	}
	input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
	    color: nth($grey, 5);
	}
	input[type="text"]:-ms-input-placeholder { /* Internet Explorer 10+ */
	    color: nth($grey, 5);
	}
}

.main-container{
	ul{
		margin-bottom: 0;
		position: relative; /*required for submenu overflow visibility*/

		li{
			line-height: 48px;
			padding: 0 2%;
			overflow: hidden;

			svg{
				height: 12px;
				// width: 18px;
				vertical-align: middle;
			}
		}
	}
}

.scroll-container{
	height: 75.25vh;
	overflow: scroll;
}

/*Column widths & floats*/
.li-col1{
	float: left;
}

.li-col1, .li-col7{
	width: 6%;
	display: inline-block;
}

.li-col2{
	display: inline-block;
	@include col(4);
	float: left;
}

.li-col3, .li-col4{
	@include col(2);
	display: inline-block;
	float: left;
}

.li-col6, .li-col5{
	@include col(1);
	float: right;
	display: inline-block;
}

.li-col7{
	float: right;
	margin-top: 18px;

	svg{
		height: 12px;
		width: 12px;
		margin-right: 30%;
	}
}

.li-header{
	font-weight: 700;
	@include col(12);
	border-bottom: 1px solid nth($grey, 4);
	font-size: .8em;

	.li-col1{
		// height: 21px;
		line-height: 18px;
		font-size: .6em;
		padding-top: 19px;
		color: nth($grey, 3);

		svg{
			display: block;
		}
	}

	.li-col7{
		text-align: right;
		margin-top: 0;
	}
}

.li-group{
	color: nth($grey, 2);
	font-size: .8em;
	cursor: pointer;
}

li.li-group:nth-child(even){
	background: nth($grey, 6);
}













/*=====================================*/
/*li dropdown==========================*/
/*=====================================*/

li.li-group.dropdown{
	background: #fff;
	padding: 18px 18px 18px 9px;
	overflow: hidden;
	min-height: 240px;

	img{
		@include col(10);
		margin: 40% 0 0 9px;
		border-radius: 27px;
	}

	.li-desc{
		line-height: 18px;
		margin-left: 9px;
		display: inline-block;
		@include col(6);
	}
}

/*Right side menu in dropdown*/
ul.dropdown-main-menu{
	@include col(2);
	float: right;

	li{
		font-size: .7em;
		line-height: 27px;
	}

	li.li-share{
		margin: 18px 0 0 18px;

		svg{
			height: 21px;
		}
	}
}



/*=====================================*/
/*dropdown-main-menu-popouts===========*/
/*=====================================*/
ul.dropdown-main-menu{
	li{/*do not write styles here. Write them in the above ul.dropdown-main-menu li*/
		ul{
			width: 110%;
			position: absolute;
			right: 105%;
			height: 200px;
			overflow: auto;
			padding: 6% 0 0 6%;
			border-left: 1px solid nth($grey, 6);

			li{/*submenu level*/
				form{
					margin-bottom: 9px;
					margin-top: 0;
					padding-right: 12%;/*mimicks popout padding*/

					input{
						height: 24px;
						font-size: .6em;
						margin: 0;
						margin-bottom: 3px;
					}

					input[type="submit"]{
						line-height: 0px;
						background: $red;
						color: $white;
					}
				}
			}
		}
	}
}

ul.dropdown-main-menu li ul::-webkit-scrollbar {
    display: none;
}

.popout{
	font-size: .8em;
}

.add-to-playlist-menu{
	background: none;/*overrides popout styles*/
	box-shadow: none;
	padding-right: 0;/*orig: 6% Removed to allow right arrow to hit edge*/

	svg{/*more arrow right*/
		float: right;
	}

	p{
		margin-bottom: 6px;
	}

	li.playlist-menu-sub-list{
		font-size: 1.2em;
		line-height: 26px;
	}
}


.playlist-menu-header{
	border-bottom: 1px solid nth($grey, 6);
	margin-bottom: 3px;
}

