/*=====================================*/
/*li.li-group.dropdown{*/
.dropdown{
	/*background: #fff;*/
	padding: 18px 18px 18px 9px;
	overflow: hidden;
	min-height: 240px;
	display: none;//Added to experiment with jquery show/hide on dropdowns
	@include col(12);
	position: relative;

	.youtube-img{//youtube thumbnail
		width: 100%;
		/*height: 100%;*/
		min-height: 102px;
		max-height: 302px;
	}

	.li-desc{
		line-height:1.5;
		font-family: OpenSans;
		font-size: 1.5em;
		margin-left: 9px;
		display: inline-block;
		@include col(6);
	}

	.li-col1{
		@include col(2);
		max-height: 100%;
	}

}

/*Right side menu in dropdown*/
ul.dropdown-main-menu{
	@include col(2);
	float: right;

	li{
		font-size: .7em;
		line-height: 27px;

		.addedToPlaylistCheck{
			height: 12px !important;//overrides main li specificity. Used in jquery affordance
		}
	}

	li.li-share{
		margin: 9px 0 0 24px;
		font-size: .6em;

		.share-strip{
			img.svg:first-child{
				padding-left: 0;
				width: 21px;
			}
		}

		svg, img{
			height: 21px;
			margin-right: 6px;
		}
	}

	li.li-buy{
		svg, img{
			height: 36px;
		}
		margin:0 0 0 18px;
	}
}



