/*=====================================*/
/*Footer Container=====================*/
/*=====================================*/
.footer{
	background: $black;
	height: 72px;
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 1001;
	padding: 0 2%;
	color: $white;
	// overflow: hidden;

	// -webkit-box-shadow: 0px -60px 27px -7px rgba(235,235,235,.8);//$white
