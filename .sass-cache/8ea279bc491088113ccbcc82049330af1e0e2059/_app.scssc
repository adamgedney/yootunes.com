// mobile slideout class
.sidebarSlideRight{
	left: 0 !important;
	@include duration;
}

.sidebarSlideLeft{
	left: -66.6666666% !important;
	@include duration;
}

aside.app{
	position: absolute;
	z-index: 998;
	/*display: none;*/
