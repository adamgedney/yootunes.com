.share-strip{
	overflow: hidden;

	svg, img{
		@include col(3);
		display: block;
		float: left;
		padding-left: 6px;
	}
}


.li-group:hover, .li-playlist:hover, .library-nav li:hover{
	background: nth($grey, 5);
}


svg:hover, img:hover{
	opacity: .8;
}

//Borowed from CSS-Tricks
.truncate {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

input[type="password"]{
	line-height: 16px;
}

/*--------------------------------------End Objects / Start Main------------------------------------*/

.no-select{
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: -moz-none;
    -o-user-select: none;
    user-select: none;
}

*{
	@extend .no-select;
}


