/*--------------------------------------End Objects / Start Main------------------------------------*/

.no-select{
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: -moz-none;
    -o-user-select: none;
    user-select: none;
}

img, .section-header, aside.app, .li-header, .li-group, .footer{
	@extend .no-select;
}



html{
	background: $white;
}

//body#app
#app{
	background: $white;
	overflow: hidden;
	letter-spacing: 1px;/*give gothic font some space*/
}


#mobile-header{
	height: 48px;
