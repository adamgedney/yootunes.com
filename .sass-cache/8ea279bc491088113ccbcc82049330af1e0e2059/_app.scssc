		@include app-break-smmd{
			height: 72px
		}
	}

	input[type="text"]{
		@include col(10);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 1.2em;
		font-weight: 100;
		/*background: transparent;*/
