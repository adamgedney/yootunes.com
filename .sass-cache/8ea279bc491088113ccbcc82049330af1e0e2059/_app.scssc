#test-frame{
	width: 1000px;
	height: 590px;
	border: 1px solid black;
}

.info-wrapper{
	overflow: hidden;
	padding: 6%;
	position: relative;

	input[type="text"], input[type="password"], input[type="submit"], select, button{
		height: 36px;
	}

	input[type="submit"], button{
		@include col(12);
		height: 45px;
		background: $blue;
		color: $white;
		line-height: 16px;
		border: none;
	}

	label{
		display: block;
		font-size: 1.2em;
		color: nth($grey, 2);
	}

	.second-form{
		margin-top: 36px;
	}
}

.info-col1, .info-col2, .info-col3{
	@include col(4);
	height: 100%;
	float: left;

	p{
		border-bottom: 1px solid nth($grey, 5);
	}
}

.info-col1{
	overflow: hidden;

	select{
		background: #fff;
		@include col(12);
		padding: 5px;
		font-size: 16px;
		line-height: 1;
		border: 0;
		border-radius: 2px;
		height: 36px;
		-webkit-appearance: none;
		border: 1px solid nth($grey, 3);
		margin-bottom: 9px;
	}
}

#infoId{
	float: right;
	margin-top: 9px;
}

.info-col3{
	padding-left: 6%;


	label[for="emailUpdatesInfo"], input[type="checkbox"]{
		display: inline;
		vertical-align: middle;
	}
}

#logoutLink{
	margin-top: 36px;
}

#deleteAccount{
	background: $red;
}


.info-col2{
	padding-left: 6%;

	ul{
		li{
			overflow: hidden;
			border-top: 1px solid nth($grey, 3);
			line-height: 36px;

			img{
				height: 12px;
				float: right;
				margin-top: 10px;
			}
		}
	}
}










