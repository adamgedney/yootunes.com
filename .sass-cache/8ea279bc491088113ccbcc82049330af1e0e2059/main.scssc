/*


----------------SASS------------------*/

$white: #ebebeb;
$grey: #272a2a, /* slightly lighter ->2a2d2d*/
#494F4F,
#949494,
#d6d6d6,
#e2e2e2,
#e7e7e7;
$dark-grey: #343838;
$black: #0f1010;
$blue: #3b578a;
$red: #cf2425;
$yellow: #e5a539;
$green: #2f7c2f;

/*----Sass imports----*/
@import "util";
@import "reset";
@import "defaults";




/*--------------------------------------End General / Start Objects------------------------------------*/


.hide-text {
  text-indent: 200%;
  overflow: hidden;
  white-space: nowrap;
}

.small{
	font-size: .4em;
}

.super{
	vertical-align: top;
}

.bg-white{
	background: #fff;
	@include duration();
}

.red {
  color: $red !important;//assigned ! for overriding specificity through jquery operations
  @include duration;
}

.blue {
  color: $blue;
}

.green{
	color: $green;
}

.light-gray {
  color: nth($grey, 3);
}

.mid-gray{
	color: nth($grey, 2);
}

.dark-gray {
  color: nth($grey, 1);
}

.underline {
  text-decoration: underline;
}

.outline{
	border: 1px solid $white;
	border-radius: 2px;
}

.light{
	font-weight: 100;
}

.bold{
	font-weight: 700;
}

input[type="email"], input[type="text"], input[type="submit"], input[type="password"], .copy-signup, select{
	height: 48px;
	@include col(12);
}


input[type="submit"], .copy-signup{
	border: none;
	background: $yellow;
	color: nth($grey, 1);
	@include col(12);
}

.error-afford{
	position: absolute;
	top: 3px;
	color: $red;
	font-size: 1em;
}
/*--------------------------------------End Objects / Start Main------------------------------------*/


#wrapper{
	background: $white;
}


.popdown{
	@include col(10);
	position: absolute;
	right: 0;
	top: 54px;
	margin-right: 6%;
	min-height: 190px;
	background: $white;
	border-radius: 2px;
	border: 1px solid nth($grey, 3);
	padding: 2% 1%;

	@include break-md{
		@include col(2);
	}

	&:before{
		content:"";
		width: 0;
		height: 0;
		border-left: 9px solid transparent;
		border-right: 9px solid transparent;
		border-bottom: 18px solid $white;
		position: absolute;
		top: -9px;
		right: 27px;
	}

	input, button{
		height: 36px;
	}
}






header {
  height: 60px;
  background: $black;
  padding: 0 6%;
  // overflow: hidden; //********Need to fix this to accomidate floats in children*/
  position: fixed;
  left: 0;
  right: 0;
  z-index: 100;

  img{
  	margin-top: 6px;
  }

  #sign-in-btn{
  	background: none;
  	float: right;
  	color: $white;
  	line-height: 0;
  	font-size: 1.5em;
  	font-weight: 100;
  	margin-top: 15px;
  }


}

