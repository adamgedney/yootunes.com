}


.info{
	@include col(3);
	float: left;
	overflow: hidden;
	// border-right: 1px solid nth($grey, 1);

	ul{
		float: left;
		margin: 9px 0 0 2%;

		li{
			font-size: .8em;
			overflow: hidden;
			font-size: .5em;
		}
	}

	svg, img{
		display: block;
		float: left;
		height: 36px;
		width: 27px;
		margin: 18px 18px 0 0;
	}
}

.transport-ctrl{
	@include col(1);
	float: left;
	overflow: hidden;
	// height: 18px;
	height: 100%;
	// border-right: 1px solid nth($grey, 1);

	svg, img{
		@include col(4);
		float: left;
		cursor: pointer;
		@include vertical-align;
	}

	.play{
		max-height: 12px;
		max-width: 12px;
		margin: 0 6px 0 9px;
	}

